doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Index
    body 
        main 
            section 
                h1 Collections
                if !collections.length
                    p Collections empty
                else
                    ul 
                        each item in collections
                            li= item.name